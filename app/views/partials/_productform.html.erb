<%= f.simple_fields_for :product do |ff| %>
  <%= ff.input :breed %>
  <br>
  <%= ff.input :dob, label: "Date of Birth" %>
  <br>
  <%= ff.input :description %>
  <br>
  <%= ff.input :price %>
  <br>
  <%= ff.label "Gender" %>
  <%= ff.select :gender, ["Male", "Female"]%>
  <br>
  <%= ff.label "Quantity" %>
  <%= ff.select :quantity, [1,2,3,4,5]%>
  <br>

  <%= ff.simple_fields_for :product_images do |fff| %>
  <%= fff.label :image %>
  <%= fff.file_field :image %>
  <% end %>
<% end %>
